<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets = "@css/player.css"  prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="is.vidmot.ListiController">
    <children>
        <VBox layoutX="252.0" layoutY="38.0" prefHeight="297.0" prefWidth="209.0" spacing="10">
            <children>
                <HBox prefHeight="241.0" prefWidth="209.0" spacing="10">
                    <children>
                        <ListView fx:id="fxListView" onMouseClicked="#onValidLag" prefHeight="241.0" prefWidth="220.0" />
                    </children>
                </HBox>
                <VBox prefHeight="45.0" prefWidth="209.0">
                    <children>
                        <HBox prefHeight="55.0" prefWidth="209.0" spacing="5">
                            <children>
                                <VBox prefHeight="70.0" prefWidth="168.0">
                                    <children>
                                        <ImageView fx:id="fxMyndLagView" fitHeight="30.0" fitWidth="168.0" />
                                        <ProgressBar fx:id="fxProgressBar" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="33.0" prefWidth="168.0" progress="0.03" />
                                    </children>
                                </VBox>
                                <Button styleClass="image-button" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onPlayPause" prefHeight="30.0" prefWidth="26.0">
                                    <graphic>
                                        <ImageView fx:id="fxPlayPauseView" fitHeight="20" fitWidth="20">
                                            <Image url="@images/pause.png" />
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </VBox>
      <Button onAction="#onHeim"  layoutX="252.0" layoutY="344.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="209.0" text="Heim" />
    </children>
</AnchorPane>
